//这是第二章"C语言概述"的最后一个作业2.8。
//这串代码的功能是从一数到五。
#include <stdio.h>
void one_three(void);
void two(void);

int main(void)
{
                printf("starting now:\n");
                one_three();
                printf("two\n");
                two();
                printf("done!\n");

        return 0;
}

void one_three(void)
{
                printf("one\n");
}

void two(void)
{
                printf("three\n");
}
    